#! /bin/bash
# Get new project list.
git checkout dev
cd script
make
make run
# Copy new project list to parent dir
cd ..
cp web/js/projects.js ../projects.js
git checkout .
# Change to master branch
git checkout master
# Replace project list
cp ../projects.js js/projects.js
# Push changes
git add js/projects.js
git commit -m "Update projects list."
git push origin master

echo DONE
